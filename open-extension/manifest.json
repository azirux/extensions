{
    "name":"open",
    "manifest_version":2,
    "version":"1",
    "description":"Open a defined list of websites",
    "browser_action":{
        "default_popup":"popup.html",
        "default_title":"Open list of urls"
    },
    "permissions":[
        "tabs"
    ],
    "background":{
        "scripts":[
            "background.js"
        ]
    }
}